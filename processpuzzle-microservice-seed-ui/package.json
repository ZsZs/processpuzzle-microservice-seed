{
  "name": "processpuzzle-microservice-seed-ui",
  "version": "0.1.1",
  "description": "ProcessPuzzle Microservice UI Seed",
  "main": "./target/dist/index.umd.js",
  "module": "./target/dist/index.js",
  "typings": "./target/dist/index.d.ts",
  "license": "Apache",
  "keywords": [
    "angular",
    "angular5",
    "aot",
    "aot-compatible",
    "aot-compilation",
    "library",
    "ng",
    "seed",
    "starter",
    "typescript"
  ],
  "author": {
    "name": "Zsolt Zsuffa",
    "url": "https://www.linkedin.com/in/zsoltzsuffa/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ZsZs/processpuzzle-microservice-seed.git"
  },
  "bugs": {
    "url": "https://github.com/ZsZs/processpuzzle-microservice-seed/issues"
  },
  "homepage": "https://github.com/ZsZs/processpuzzle-microservice-seed#readme",
  "scripts": {
    "build": "npm run build:esm && npm run build:umd",
    "build-dev": "npm run ci",
    "build:esm": "gulp inline-templates && npm run ngcompile",
    "build:esm:watch": "gulp build:esm:watch",
    "build:umd": "webpack --config webpack-umd.config.ts --mode production",
    "build:umd:watch": "npm run build:umd -- --watch",
    "build:watch": "concurrently --raw \"npm run build:umd:watch\" \"npm run build:esm:watch\"",
    "ci": "npm run lint && npm run test && npm run build && npm run docs",
    "clean:all": "npm run clean:tmp && rimraf node_modules",
    "clean:tmp": "rimraf ./target/coverage ./target/dist ./target/tmp ./target/docs",
    "docs": "compodoc -p tsconfig.json -d ./target/docs --disableCoverage",
    "explorer": "source-map-explorer ./target/dist/index.umd.js",
    "gh-pages": "rimraf ./target/docs && npm run docs && gh-pages -d ./target/docs",
    "lint": "ng lint --type-check --format json > target/issues.json",
    "lint-watch": "ng lint --type-check",
    "ngcompile": "node_modules/.bin/ngc -p tsconfig-aot.json",
    "postversion": "git push && git push --tags",
    "prebuild": "rimraf ./target/dist ./target/tmp",
    "prebuild:watch": "rimraf ./target/dist ./target/tmp",
    "prepublishOnly": "npm run ci",
    "preversion": "npm run ci",
    "test": "karma start",
    "test:watch": "karma start --auto-watch --no-single-run"
  },
  "dependencies": {},
  "peerDependencies": {
    "@angular/common": "^5.2.9",
    "@angular/core": "^5.2.9"
  },
  "devDependencies": {
    "@angular/cli": "^1.7.3",
    "@angular/common": "^5.2.9",
    "@angular/compiler": "^5.2.9",
    "@angular/compiler-cli": "^5.2.9",
    "@angular/core": "^5.2.9",
    "@angular/platform-browser": "^5.2.9",
    "@angular/platform-browser-dynamic": "^5.2.9",
    "@compodoc/compodoc": "^1.0.9",
    "@types/jasmine": "^2.8.6",
    "@types/karma": "^1.7.3",
    "@types/node": "^9.4.7",
    "@types/tapable": "1.0.2",
    "@types/webpack": "^4.1.1",
    "@types/webpack-env": "^1.13.5",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^5.0.0-1",
    "codecov": "^3.0.0",
    "codelyzer": "^4.2.1",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.11",
    "gh-pages": "^1.1.0",
    "gulp": "^3.9.1",
    "gulp-inline-ng2-template": "^4.1.0",
    "istanbul-instrumenter-loader": "^3.0.0",
    "jasmine": "^3.1.0",
    "jasmine-core": "^3.1.0",
    "jasmine-expect": "^3.8.3",
    "jasmine-spec-reporter": "~4.2.1",
    "json-loader": "^0.5.7",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-coverage-istanbul-reporter": "^1.4.2",
    "karma-jasmine": "^1.1.1",
    "karma-jasmine-html-reporter": "^1.0.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-remap-coverage": "^0.1.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^3.0.0",
    "node-sass": "^4.7.2",
    "npm-bump": "0.0.23",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.2",
    "rxjs": "^5.5.7",
    "sass-loader": "^7.0.1",
    "source-map-explorer": "^1.5.0",
    "to-string-loader": "^1.1.5",
    "ts-node": "^6.0.0",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2",
    "webpack": "^4.2.0",
    "webpack-angular-externals": "^1.0.2",
    "webpack-cli": "^2.0.13",
    "webpack-rxjs-externals": "^2.0.0",
    "zone.js": "git://github.com/JiaLiPassion/zone.js.git#jasmine-dist"
  }
}
